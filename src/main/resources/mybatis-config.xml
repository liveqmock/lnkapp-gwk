<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
    PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
    <properties resource="db.properties"/>

    <environments default="development">
        <environment id="${environment}">
            <transactionManager type="${transaction}"/>
            <dataSource type="${pool}">
                <property name="driver" value="${ConnectionManager.driver}"/>
                <property name="url" value="${ConnectionManager.connection}"/>
                <property name="username" value="${ConnectionManager.user}"/>
                <property name="accredit_code" value="${ConnectionManager.accredit_code}"/>
            </dataSource>
        </environment>
    </environments>
    <mappers>
        <mapper resource="org/fbi/gwk/repository/mapper/CpSequenceMapper.xml"/>
        <mapper resource="org/fbi/gwk/repository/mapper/LsBdgagencyMapper.xml"/>
        <mapper resource="org/fbi/gwk/repository/mapper/LsBdgagencyHMapper.xml"/>
        <mapper resource="org/fbi/gwk/repository/mapper/LsCardbaseinfoMapper.xml"/>
        <mapper resource="org/fbi/gwk/repository/mapper/LsCardstatusMapper.xml"/>
        <mapper resource="org/fbi/gwk/repository/mapper/LsConsumeinfoMapper.xml"/>
        <mapper resource="org/fbi/gwk/repository/mapper/LsPaybackinfoMapper.xml"/>
        <mapper resource="org/fbi/gwk/repository/mapper/LsPersonalinfoMapper.xml"/>
        <mapper resource="org/fbi/gwk/repository/mapper/LsSysinfoMapper.xml"/>
        <mapper resource="org/fbi/gwk/repository/mapper/LsTaskinfoMapper.xml"/>
        <mapper resource="org/fbi/gwk/repository/mapper/OdsbCrdCrtAcctMapper.xml"/>
        <mapper resource="org/fbi/gwk/repository/mapper/OdsbCrdCrtMapper.xml"/>
        <mapper resource="org/fbi/gwk/repository/mapper/OdsbCrdCrtTradMapper.xml"/>
    </mappers>

</configuration>